{% if items|length > 0 %}
    <table class="table">
        <thead>
        <tr>
            <th scope="col" style="width:20%">#</th>
            <th scope="col" style="width:15%">Id</th>
            <th scope="col" style="width:15%">ФИО</th>
            <th scope="col" style="width:20%">Кол-во отработанных часов (без учета подчиненных/ с учетом)</th>
            <th scope="col" style="width:15%">E-mail</th>
            <th scope="col" style="width:15%"></th>
        </tr>
        </thead>
        <tbody>
            {% import 'time_sheet_result_tree.twig' as time_sheet_result_tree %}

            {{ time_sheet_result_tree.list(items) }}

        </tbody>
    </table>
{% endif %}
